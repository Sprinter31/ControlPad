<ui:FluentWindow x:Class="ControlPad.Windows.Popups.SelectKeyPopup"
        xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlPad.Windows.Popups"
        mc:Ignorable="d"
        Title="SelectKeyPopup" Height="300" Width="300">
    <DockPanel>
        <ui:TitleBar DockPanel.Dock="Top" x:Name="TitleBarSelectKeyPopup"
        Title="Please Select a key"
        Icon="{ui:ImageIcon 'pack://application:,,,/Resources/logo.ico'}"/>

    </DockPanel>
</ui:FluentWindow>
